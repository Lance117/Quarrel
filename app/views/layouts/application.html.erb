<!DOCTYPE html>
<html>
  <head>
    <title>Flack</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.whiteLogo = "<%= image_url('Slack_RGB_White.png') %>";
      window.normLogo = "<%= image_url('Slack_RGB.png') %>";
      window.teamsPic = "<%= image_url('teams_pic.png') %>";
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
